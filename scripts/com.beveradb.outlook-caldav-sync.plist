<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.beveradb.outlook-caldav-sync</string>
    <key>ProgramArguments</key>
    <array>
        <string>/Users/yourname/Projects/calendar-sync/run_sync.sh</string>
    </array>
    <key>WorkingDirectory</key>
    <string>/Users/yourname/Projects/calendar-sync</string>
    <key>StartCalendarInterval</key>
    <dict>
        <key>Hour</key>
        <integer>17</integer>
        <key>Minute</key>
        <integer>16</integer>
    </dict>
    <key>StandardOutPath</key>
    <string>/Users/yourname/Projects/calendar-sync/sync_launchd.log</string>
    <key>StandardErrorPath</key>
    <string>/Users/yourname/Projects/calendar-sync/sync_launchd.log</string>
    <key>RunAtLoad</key>
    <true/>
</dict>
</plist>
